@page
@model CategoryModel
@{
    ViewBag.Title = "类型管理";
    ViewBag.Current = "users.logs";
}

<div class="table-responsive">
    <table class="table table-striped">
        <tbody class="data-content">
            @foreach (var type in Model.Types)
            {
                <tr>
                    <td><gt:icon-event-type type="@type.Id"></gt:icon-event-type> @type.Name</td>
                </tr>
            }
            <gt:warning-tabled class="text-secondary" attach="Model.Types">没有添加日志类型！</gt:warning-tabled>
        </tbody>
    </table>
</div>